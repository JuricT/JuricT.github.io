{
    "sass.enabled": false,
    "sass.compiler": "libsass",
    "path": {
        "project/scss/style.scss": {
            "sass.enabled": true,
            "sass.options": {
                "includePaths": [
                    "../bower_components/bourbon/app/assets/stylesheets/"
                ],
                "outputDir": "../../build/css/",
                "imagePath": null,
                "sourceComments": false,
                "outputStyle": "nested"
            },
            "linting.collapsed": false,
            "useTabChar": false
        }
    }
}